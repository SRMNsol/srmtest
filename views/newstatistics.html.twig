  {% extends 'common/layout.html.twig' %}

{% block content %}
<div class="page-header">
  <h1>New User<small>List</small></h1>
</div>

<div class="row">
  <div class="col-md-6">
    Total  {{ neweuser|length }} New Users
  </div>
</div>

<br>
<div class="row">
  <div class="col-md-12">
    <table id="example" class="table table-bordered">
      <thead>
        <tr>
          <th class="col-md-4">User Id</th>
          <th class="col-md-4">User Alias</th>
          <th class="col-md-4">User Email</th>
        </tr>
      </thead>
      </tbody>
        {% for user in neweuser %}
        <tr>
          <td>{{ user.uid }}</td>
          <td>{{ user.alias }}</td>
          <td><a href="{{ path('user_info', {'user_search[email]': user.email }) }}" >{{ user.email }}</a></td>
                  
      
        </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>






{% endblock %}