  {% extends 'common/layout.html.twig' %}

{% block content %}
<div class="page-header">
  <h1>Cashback <small>List</small></h1>
</div>

<div class="row">
  <div class="col-md-6">
    Total  {{ cashuser|length }} Cash back
  </div>
</div>

<br>
<div class="row">
  <div class="col-md-12">
    <table id="example" class="table table-bordered">
      <thead>
        <tr>
          <th class="col-md-4">User Id</th>
          <th class="col-md-4">User Email</th>
          <th class="col-md-4"> Cashback</th>
        </tr>
      </thead>
      </tbody>
        {% for user in cashuser %}
        <tr>
          <td>{{ user.userid }}</td>
            <td><a href="{{ path('user_info', {'user_search[email]': user.email }) }}" >{{ user.email }}</a></td>
            <td>{{user.cashback}}</td>
                  
      
        </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>






{% endblock %}